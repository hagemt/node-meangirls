{
  "name": "meangirls",
  "version": "0.1.0",
  "private": true,
  "author": "Tor Hagemann <tor.hagemann@gmail.com>",
  "description": "JavaScript re-implementation of Aphyr's gem for CRDTs",
  "homepage": "https://github.com/hagemt/meangirls#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hagemt/meangirls/issues"
  },
  "devDependencies": {
    "eslint": "^3",
    "nyc": "^10",
    "should": "^11",
    "mocha": "^3"
  },
  "eslint": {
    "extends": "eslint:recommended"
  },
  "keywords": [
    "Convergent",
    "Replicated",
    "Data",
    "Types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hagemt/meangirls.git"
  },
  "scripts": {
    "coverage": "nyc",
    "eslint": "eslint **/*.js",
    "test": "npm run coverage -- --reporter text-summary mocha # see test",
    "test:report": "npm run coverage -- --reporter html mocha # see coverage",
    "test:watch": "watch --color 'npm test -- --colors --reporter min'",
    "posttest": "npm run eslint"
  }
}
